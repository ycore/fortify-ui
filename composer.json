{
    "name": "ycore/fortify-ui",
    "description": "A Laravel Fortify-powered authentication UI",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Meyer",
            "email": "y-core@outlook.com"
        }
    ],
    "homepage": "https://github.com/ycore/fortify-ui",
    "keywords": ["Laravel", "Fortify", "laravel-fortify", "laravel-ui", "FortifyUI", "fortify-ui"],
    "require": {
        "laravel/fortify": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ycore\\FortifyUITailwind\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ycore\\FortifyUITailwind\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ycore\\FortifyUITailwind\\FortifyUIServiceProvider"
            ]
        }
    }
}
